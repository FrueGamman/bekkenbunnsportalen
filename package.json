{
  "name": "in-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.app.json --noEmit && vite build",
    "lint": "eslint .",
    "lint:a11y": "eslint . --ext .ts,.tsx --rule 'jsx-a11y/alt-text: error' --rule 'jsx-a11y/anchor-has-content: error' --rule 'jsx-a11y/aria-props: error'",
    "preview": "vite preview",
    "test": "sh -c 'if [ \"$CI\" = \"true\" ]; then echo \"Skipping unit tests on CI\"; exit 0; else jest; fi'",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "sh -c 'if [ \"$CI\" = \"true\" ]; then echo \"Skipping e2e tests on CI\"; exit 0; else playwright test; fi'",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:a11y": "playwright test tests/e2e/accessibility.spec.ts",
    "test:all": "npm run test && npm run test:e2e",
    "audit": "npm audit",
    "audit:content": "cd tools/audit && python crawl.py && python compare.py && python export_map.py"
  },
  "dependencies": {
    "@mdx-js/react": "^3.1.1",
    "@mdx-js/rollup": "^3.1.1",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@types/mdx": "^2.0.13",
    "@vercel/analytics": "^1.6.1",
    "lucide-react": "^0.563.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-responsive": "^10.0.1",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.1",
    "@eslint/js": "^9.39.2",
    "@jest/core": "^30.2.0",
    "@jest/reporters": "^30.2.0",
    "@jest/transform": "^30.2.0",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.19.11",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "axe-core": "^4.11.1",
    "babel-jest": "^30.2.0",
    "baseline-browser-mapping": "^2.9.19",
    "eslint": "^9.39.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^16.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-circus": "^30.2.0",
    "jest-cli": "^30.2.0",
    "jest-config": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-runner": "^30.2.0",
    "jest-runtime": "^30.2.0",
    "jest-snapshot": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1"
  },
  "overrides": {
    "test-exclude": "^7.0.0"
  }
}
