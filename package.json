{
  "name": "in-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "22.x",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.app.json --noEmit && vite build",
    "lint": "eslint .",
    "lint:a11y": "eslint . --ext .ts,.tsx --rule 'jsx-a11y/alt-text: error' --rule 'jsx-a11y/anchor-has-content: error' --rule 'jsx-a11y/aria-props: error'",
    "preview": "vite preview",
    "test": "sh -c 'if [ \"$CI\" = \"true\" ]; then echo \"Skipping unit tests on CI\"; exit 0; else jest; fi'",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "sh -c 'if [ \"$CI\" = \"true\" ]; then echo \"Skipping e2e tests on CI\"; exit 0; else playwright test; fi'",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:a11y": "playwright test tests/e2e/accessibility.spec.ts",
    "test:all": "npm run test && npm run test:e2e",
    "audit": "npm audit",
    "audit:content": "cd tools/audit && python crawl.py && python compare.py && python export_map.py"
  },
  "dependencies": {
    "@mdx-js/react": "^3.1.1",
    "@mdx-js/rollup": "^3.1.1",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@types/mdx": "^2.0.13",
    "@vercel/analytics": "^1.6.1",
    "lucide-react": "^0.563.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-responsive": "^10.0.1",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@eslint/js": "^9.25.0",
    "@jest/core": "^29.7.0",
    "@jest/reporters": "^29.7.0",
    "@jest/transform": "^29.7.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.4.1",
    "axe-core": "^4.10.3",
    "babel-jest": "^29.7.0",
    "baseline-browser-mapping": "^2.9.19",
    "eslint": "^9.25.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-config": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "jest-runner": "^29.7.0",
    "jest-runtime": "^29.7.0",
    "jest-snapshot": "^29.7.0",
    "ts-jest": "^29.4.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1"
  }
}
